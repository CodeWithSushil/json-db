{
    "name": "jsondbphp/jsondb",
    "description": "JsonDB: Lightweight Document Container for Modern PHP Apps.",
    "type": "library",
    "keywords": [
      "jsondb",
      "jsondbphp",
      "json database",
      "database"
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "laravel/pint": "^1.24",
        "myclabs/deep-copy": "^1.13",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.2",
        "symfony/var-dumper": "^7.3"
    },
    "license": "MIT",
    "autoload": {
      "psr-4": {
        "Json\\Database\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sushil Kumar",
            "email": "sushilkumbhar77@gmail.com"
        }
    ],
    "scripts": {
        "lint": "vendor/bin/pint",
        "unit": "vendor/bin/phpunit",
        "type": "vendor/bin/phpstan"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true   
}
